# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

#League
GET     /league                       controllers.LeagueController.home
POST    /league/add                      controllers.LeagueController.add
GET     /league/delete/:id                 controllers.LeagueController.delete(id: Long)